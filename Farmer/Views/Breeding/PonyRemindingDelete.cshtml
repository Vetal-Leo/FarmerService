@model Domain.Entities.Breeding.BreedingReminder

@{
    ViewBag.Title = "Коневодство(Пони) - Удаление напоминаний";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<h4 class="georgia-handler">Сервис Животноводство.</h4>
<h3 class="academy-camel">Коневодство(Пони). Напоминания.</h3>
<h3>Вы уверены, что хотите это удалить &nbsp; @Model.AnimalName ?</h3>
<h4>Удаление напоминания :</h4>
<h6>@Model.Text</h6>
<div>
    @using (Html.BeginForm())
    {
        @Html.AntiForgeryToken()
        <div>
            <input type="submit" value="Удалить" class="btn btn-primary" />
        </div>
        <br />
        @Html.ActionLink("Назад в Коневодство(Пони). Напоминания.", "PonyRemindings")
    }

</div>


