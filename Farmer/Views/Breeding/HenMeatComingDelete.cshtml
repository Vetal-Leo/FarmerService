@model Domain.Entities.Breeding.BreedingComings

@{
    ViewBag.Title = "Куры (Мясные и Мясояичные породы) - Удаление животных.";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<h4 class="georgia-handler">Сервис Животноводство.Птицеводство.</h4>
<h3 class="academy-camel">Куры (Мясные и Мясояичные породы). Приход.</h3>
<h3>Вы уверены, что хотите это удалить  &nbsp; @Model.Name , @Model.Amount голов ?</h3>
<div>
    @using (Html.BeginForm())
    {
        @Html.AntiForgeryToken()
        <div>
            <input type="submit" value="Удалить" class="btn btn-primary" />
        </div>
        <br />
        @Html.ActionLink("Назад в Куры (Мясные и Мясояичные породы). Приход", "HenMeatComing")
    }

</div>
