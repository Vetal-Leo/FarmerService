@model Domain.Entities.Breeding.BeeComings

@{
    ViewBag.Title = "Пчеловодство - Удаление животных.";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<h4 class="georgia-handler">Сервис Животноводство.</h4>
<h3 class="academy-bee">Пчеловодство. Приход.</h3>
<h3>Вы уверены, что хотите это удалить  &nbsp; @Model.Name , @Model.Amount улей ?</h3>
<div>
    @using (Html.BeginForm())
    {
        @Html.AntiForgeryToken()
        <div>
            <input type="submit" value="Удалить" class="btn btn-primary" />
        </div>
        <br />
        @Html.ActionLink("Назад в Пчеловодство. Приход", "BeeComing")
    }

</div>
