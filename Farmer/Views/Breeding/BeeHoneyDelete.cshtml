@model Domain.Entities.Breeding.BeeHoney

@{
    ViewBag.Title = "Пчеловодство - Удаление мёда.";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<h4 class="georgia-handler">Сервис Животноводство.</h4>
<h3 class="academy-bee">Пчеловодство. Сбор мёда.</h3>
<h3>Вы уверены, что хотите это удалить  &nbsp; @Model.AnimalName  &nbsp;  @Model.Amount  &nbsp; @Model.MassType ?</h3>
<div>
    @using (Html.BeginForm())
    {
        @Html.AntiForgeryToken()
        <div>
            <input type="submit" value="Удалить" class="btn btn-primary" />
        </div>
        <br />
        @Html.ActionLink("Назад в Пчеловодство. Сбор мёда.", "BeeHoneys")
    }

</div>


